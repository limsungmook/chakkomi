$(function(){
	$('#laydesk_shop_body').fadeOut('slow', function(){
		$('#laydesk_shop_body').html('<%= escape_javascript(render('cart')) %>').fadeIn('slow');
		
		$("#back_page").click(function(){
		    history.go(-1);
		});

		$(".ajax_link").click(function(e) {
			var href = $(this).attr('href');
			$.address.value(href);
			return false;
		    });
		
	    });
    });