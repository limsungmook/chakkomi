$(function(){


	$('#laydesk_body').fadeOut('10000', function(){
		$('#laydesk_body').html('<%= escape_javascript(render('store')) %>').fadeIn('slow');

		var History = window.History;	
		if ( !History.enabled ){
		    return false;
		}
		
		$("#cart a, .product_list a").click(function() {
			History.pushState(null, document.title, this.href);
			return false;
		    });
		// 프로필은 ajax이 아닌 페이지 리로딩이기 때문에 a href 리퀘스트를 무시하지 않는다. 
		
	    });
    });